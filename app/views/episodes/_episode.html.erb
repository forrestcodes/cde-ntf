
<div class="card">
  <div class="card-header">
    <%= episode['episode'] %>
  </div>
  <div class="card-body">
    <h5 class="card-title"><%= episode['name'] %></h5>
    <p class="card-text">Air Date: <%= episode['air_date'] %></p>
  </div>
  <div class="card-footer">
    <%= link_to 'More Info', episode_path(episode['id']) %>
  </div>
</div>
